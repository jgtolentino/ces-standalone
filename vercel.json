{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "env": {
    "VERCEL_DEPLOYMENT_PROTECTION": "false",
    "NEXT_PUBLIC_DEPLOYMENT_PROTECTION": "false"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Deployment-Protection",
          "value": "disabled"
        }
      ]
    }
  ]
}