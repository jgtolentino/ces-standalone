{
  "name": "@ai/dashboard-ui",
  "version": "0.1.0",
  "description": "Shared dashboard UI components for retail analytics and insights",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components/*": "./src/components/*",
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@ai/ui": "workspace:*",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-dropdown-menu": "^2.1.3",
    "@radix-ui/react-select": "^2.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "recharts": "^2.12.7",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.6.0",
    "lucide-react": "^0.453.0",
    "framer-motion": "^11.13.1"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "tailwindcss": "^3.4.14",
    "eslint": "^8.57.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}