{
  "name": "@ai/db",
  "version": "0.1.0",
  "description": "Typed Supabase client with RLS and tenant isolation",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "generate-types": "supabase gen types typescript --local > src/types/database.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.9",
    "pg": "^8.11.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.10.9",
    "tsup": "^8.3.5",
    "eslint": "^8.57.1"
  }
}